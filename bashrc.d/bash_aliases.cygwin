# ~/.bashrc.d/bash_aliases.cygwin
#
alias ping6="/cygdrive/c/Windows/SysWOW64/PING.EXE -6"
alias start='cygstart'
alias chromeprofile='cd /cygdrive/c/Users/accdias/AppData/Local/Google/Chrome/User\ Data/Default'
alias rmtorrents='find /cygdrive/c/Users/accdias/AppData/Roaming/uTorrent/ -type f -name "*.torrent" -ls -exec rm {} \;'
alias roaming='cd /cygdrive/c/Users/accdias/AppData/Roaming/'
alias appdata='cd /cygdrive/c/Users/accdias/AppData'
