# ~/.bashrc.d/cygwin/aliases
alias start='cygstart'
alias chromeprofile="cd /cygdrive/c/Users/${USER}/AppData/Local/Google/Chrome/User\ Data/Default"
alias rmtorrents="find /cygdrive/c/Users/${USER}/AppData/Roaming/uTorrent/ -type f -name '*.torrent' -ls -exec rm {} +;"
alias roaming="cd /cygdrive/c/Users/${USER}/AppData/Roaming/"
alias appdata='cd /cygdrive/c/Users/${USER}/AppData'
alias vbox='vboxmanage'
alias vboxvms='vboxmanage list runningvms'
# EOF
