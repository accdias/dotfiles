# ~/.bashrc.d/msys/aliases
alias start='cygstart'
alias chromeprofile="cd /c/Users/${USER}/AppData/Local/Google/Chrome/User\ Data/Default"
alias rmtorrents="find /c/Users/${USER}/AppData/Roaming/uTorrent/ -type f -name '*.torrent' -ls -exec rm {} +;"
alias roaming="cd /c/Users/${USER}/AppData/Roaming/"
alias appdata="cd /c/Users/${USER}/AppData"
alias vbox='vboxmanage'
alias vboxvms='vboxmanage list runningvms'
# EOF
